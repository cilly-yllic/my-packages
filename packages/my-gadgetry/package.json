{
  "name": "my-gadgetry",
  "version": "1.3.6",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./color": "./dist/lib/modules/color.js",
    "./converters/array": "./dist/lib/modules/converters/array.js",
    "./converters/number": "./dist/lib/modules/converters/number.js",
    "./converters/string": "./dist/lib/modules/converters/string.js",
    "./dev-ops/package-generator/generate-alias": "./dist/lib/modules/dev-ops/package-generator/generate-alias.js",
    "./fs": "./dist/lib/modules/fs.js",
    "./type-check": "./dist/lib/modules/type-check.js",
    "./types/utilities": "./dist/lib/modules/types/utilities.js"
  },
  "files": [
    "dist",
    "color",
    "converters",
    "dev-ops",
    "fs",
    "type-check",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "fs-extra": "^11.3.0",
    "tslib": "^2.8.1",
    "minimatch": "^10.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4"
  }
}
